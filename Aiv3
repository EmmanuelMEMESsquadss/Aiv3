			local delta = input.Position - dragStart
			MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end
	end)
	
	-- Button Functions
	DumpBtn.MouseButton1Click:Connect(function()
		StatusLabel.Text = "ğŸ”„ Dumping game...\nPlease wait..."
		DumpBtn.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
		DumpBtn.Text = "â³ DUMPING..."
		
		task.wait(0.1)
		
		-- Start dump
		local containers = {
			{game.ReplicatedStorage, "ReplicatedStorage"},
			{game.Lighting, "Lighting"},
			{game.StarterGui, "StarterGui"},
			{game.StarterPack, "StarterPack"},
			{game.StarterPlayer, "StarterPlayer"},
			{workspace, "Workspace"}
		}
		
		for _, container in ipairs(containers) do
			scanContainer(container[1], 0)
		end
		
		-- Update UI
		StatsText.Text = string.format(
			"ğŸ“Š STATISTICS\n\nğŸ“œ Scripts: %d\nğŸ“¦ Modules: %d\nğŸ“¡ Remotes: %d\nğŸ¨ Assets: %d\nğŸ“ Remote Calls: %d",
			dumpData.Stats.Scripts,
			dumpData.Stats.Modules,
			dumpData.Stats.Remotes,
			dumpData.Stats.Assets,
			dumpData.Stats.RemoteCalls
		)
		
		StatusLabel.Text = "âœ… DUMP COMPLETE!\nReady to save"
		DumpBtn.BackgroundColor3 = Color3.fromRGB(50, 200, 100)
		DumpBtn.Text = "âœ… DUMP COMPLETE"
	end)
	
	SaveBtn.MouseButton1Click:Connect(function()
		local textOutput = generateTextOutput()
		local baseFilename = string.format("GameDump_%s_%s", game.PlaceId, os.date("%Y%m%d_%H%M%S"))
		
		-- Try text file
		local success1, path1 = tryWriteFile(baseFilename .. ".txt", textOutput)
		
		-- Try JSON file
		local success2, jsonData = pcall(function() return HttpService:JSONEncode(dumpData) end)
		local success2b, path2
		if success2 then
			success2b, path2 = tryWriteFile(baseFilename .. ".json", jsonData)
		end
		
		if success1 or success2b then
			StatusLabel.Text = "âœ… FILES SAVED!\nCheck Download folder"
		else
			StatusLabel.Text = "âŒ Save failed!\nTry clipboard instead"
		end
	end)
	
	CopyBtn.MouseButton1Click:Connect(function()
		local textOutput = generateTextOutput()
		local success = pcall(function()
			setclipboard(textOutput)
		end)
		
		if success then
			StatusLabel.Text = "âœ… COPIED TO CLIPBOARD!"
		else
			StatusLabel.Text = "âŒ Clipboard not supported"
		end
	end)
	
	ScreenGui.Parent = game:GetService("CoreGui")
	
	return ScreenGui
end

